{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course_name": {
      "type": "string"
    },
    "banner_course": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "course_description": {
      "type": "richtext"
    },
    "rating": {
      "type": "integer"
    },
    "author": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "short_description": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "status_course": {
      "type": "boolean"
    },
    "learning_progresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::learning-progress.learning-progress",
      "mappedBy": "courses"
    },
    "course_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-category.course-category"
    },
    "chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter.chapter"
    },
    "course_registrations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-registration.course-registration",
      "mappedBy": "courses"
    },
    "certificate": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::certificate.certificate",
      "mappedBy": "course"
    }
  }
}
