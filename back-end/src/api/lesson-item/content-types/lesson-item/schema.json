{
  "kind": "collectionType",
  "collectionName": "lesson_items",
  "info": {
    "singularName": "lesson-item",
    "pluralName": "lesson-items",
    "displayName": "lesson_item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "video_url": {
      "type": "string"
    },
    "learning_progresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::learning-progress.learning-progress",
      "mappedBy": "lesson_items"
    },
    "text_lesson": {
      "type": "richtext"
    },
    "document_lesson": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "lesson_lesson_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lesson-lesson-type.lesson-lesson-type",
      "mappedBy": "lesson_items"
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "lesson_items"
    }
  }
}
